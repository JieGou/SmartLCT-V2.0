<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Nova_SmartLCT_Behavior="clr-namespace:Nova.SmartLCT.Behavior;assembly=Behavior"
                    xmlns:cmd="clr-namespace:GalaSoft.MvvmLight.Command;assembly=GalaSoft.MvvmLight.Extras.WPF4"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                    xmlns:local="clr-namespace:Nova.SmartLCT.UI"
                    xmlns:localInterface="clr-namespace:Nova.SmartLCT.Interface;assembly=Nova.SmartLCT.Interface.CommonData"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:my="clr-namespace:Nova.SmartLCT.SmartLCTControl;assembly=SmartLCTControl"
                    xmlns:myNumericUD="clr-namespace:Nova.Wpf.Control;assembly=Nova.Wpf.Control.NumericUpDown"
                    xmlns:xcts="http://schemas.xceed.com/wpf/xaml/toolkit"
                    mc:Ignorable="d">
    <localInterface:LayerTypeConvertStr x:Key="layerTypeConvertStr" />
    <localInterface:EleTypeConvertLayerName x:Key="eleTypeConvertLayerName" />


    <Style TargetType="{x:Type local:ScreenPropertyPanel}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:ScreenPropertyPanel}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid Margin="0,0,0,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <GroupBox Grid.Row="0"
                                      Height="160"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Top"
                                      DataContext="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                            Path=ScannerCofigCollection}"
                                      Header="{DynamicResource Lang_PortPropPanel_AddScanner}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="32" />
                                        <RowDefinition Height="32" />
                                        <RowDefinition Height="32" />
                                        <RowDefinition Height="40" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="0"
                                               Grid.Column="0"
                                               HorizontalAlignment="Right"
                                               VerticalAlignment="Center"
                                               Text="{DynamicResource Lang_PortPropPanel_ReceiveSize}"
                                               TextWrapping="Wrap" />
                                    <ComboBox Grid.Row="0"
                                              Grid.Column="1"
                                              Margin="2,0,0,0"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Center"
                                              DisplayMemberPath="DisplayName"
                                              Foreground="#FFDCDCDC"
                                              ItemContainerStyle="{DynamicResource ComboBoxItemStyle_GuideTwo_ReceiveSize}"
                                              ItemTemplate="{DynamicResource DataTemplate_GuideTwo_ReceiveSize}"
                                              ItemsPanel="{DynamicResource ItemsPanelTemplate_GuideTwo_ReceiveSize}"
                                              ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                    Path=ScannerCofigCollection,
                                                                    Mode=TwoWay,
                                                                    UpdateSourceTrigger=PropertyChanged}"
                                              SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                     Path=SelectedScannerConfig,
                                                                     Mode=TwoWay}"
                                              Style="{DynamicResource ComboBoxStyle_GuideTwo}">
                                        <ComboBox.Background>
                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                <GradientStop Color="#FF616161" />
                                                <GradientStop Offset="1" Color="#FF585858" />
                                                <GradientStop Offset="0.495" Color="#FF616161" />
                                                <GradientStop Offset="0.5" Color="#FF585858" />
                                            </LinearGradientBrush>
                                        </ComboBox.Background>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="DropDownClosed">
                                                <Nova_SmartLCT_Behavior:NotifyControlEvent Command="{x:Static local:ScreenPropertyPanel.CustomSizeDropDownClosed}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </ComboBox>
                                    <Button Grid.Row="0"
                                            Grid.Column="2"
                                            Width="60"
                                            Height="22"
                                            Margin="5,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Command="{x:Static local:ScreenPropertyPanel.CmdShowScanBoardConfigManager}"
                                            Content="{DynamicResource Lang_PortPropPanel_Import}" />
                                    <TextBlock Grid.Row="1"
                                               Grid.Column="0"
                                               HorizontalAlignment="Right"
                                               VerticalAlignment="Center"
                                               Text="{DynamicResource Lang_PortPropPanel_AddCols}"
                                               TextWrapping="Wrap"
                                               d:LayoutOverrides="Height" />
                                    <myNumericUD:NumericUpDown Grid.Row="1"
                                                               Grid.Column="1"
                                                               Height="21"
                                                               Margin="2,0,0,0"
                                                               HorizontalAlignment="Stretch"
                                                               VerticalAlignment="Center"
                                                               Foreground="#FFDCDCDC"
                                                               IsSelectedAll="True"
                                                               MaxValue="200"
                                                               Value="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                               Path=AddColsCount,
                                                                               Mode=TwoWay,
                                                                               UpdateSourceTrigger=PropertyChanged}">
                                        <myNumericUD:NumericUpDown.Background>
                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                <GradientStop Offset="0" Color="#FF616161" />
                                                <GradientStop Offset="1" Color="#FF585858" />
                                                <GradientStop Offset="0.495" Color="#FF616161" />
                                                <GradientStop Offset="0.5" Color="#FE585858" />
                                            </LinearGradientBrush>
                                        </myNumericUD:NumericUpDown.Background>
                                    </myNumericUD:NumericUpDown>
                                    <TextBlock Grid.Row="2"
                                               Grid.Column="0"
                                               HorizontalAlignment="Right"
                                               VerticalAlignment="Center"
                                               Text="{DynamicResource Lang_PortPropPanel_AddRow}"
                                               TextWrapping="Wrap" />
                                    <myNumericUD:NumericUpDown Grid.Row="2"
                                                               Grid.Column="1"
                                                               Height="21"
                                                               Margin="2,0,0,0"
                                                               HorizontalAlignment="Stretch"
                                                               VerticalAlignment="Center"
                                                               Foreground="#FFDCDCDC"
                                                               IsSelectedAll="True"
                                                               MaxValue="200"
                                                               Value="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                               Path=AddRowsCount,
                                                                               Mode=TwoWay,
                                                                               UpdateSourceTrigger=PropertyChanged}">
                                        <myNumericUD:NumericUpDown.Background>
                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                <GradientStop Offset="0" Color="#FF616161" />
                                                <GradientStop Offset="1" Color="#FF585858" />
                                                <GradientStop Offset="0.495" Color="#FF616161" />
                                                <GradientStop Offset="0.5" Color="#FE585858" />
                                            </LinearGradientBrush>
                                        </myNumericUD:NumericUpDown.Background>
                                    </myNumericUD:NumericUpDown>
                                    <Button Name="AddSenderBut"
                                            Grid.Row="3"
                                            Grid.ColumnSpan="3"
                                            Width="75"
                                            Height="22"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            Command="{x:Static local:ScreenPropertyPanel.CmdAddScanner}"
                                            Content="{DynamicResource Lang_PortPropPanel_Add}" />
                                </Grid>
                            </GroupBox>
                            <GroupBox Grid.Row="1" Header="{DynamicResource Lang_PortPropPanel_ConnectScanner}">
                                <Grid Margin="0,0,0,0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <GroupBox Grid.Row="0"
                                              Height="220"
                                              Margin="5,7,5,5"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Stretch"
                                              DataContext="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                    Path=ScannerCofigCollection}"
                                              Header="{DynamicResource Lang_PortPropPanel_SelecteSender}">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="32" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <DockPanel LastChildFill="True">
                                                <StackPanel HorizontalAlignment="Center"
                                                            DockPanel.Dock="Right"
                                                            Orientation="Horizontal">
                                                    <Button Width="15"
                                                            Height="15"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Command="{x:Static local:ScreenPropertyPanel.CmdAddSenderIndex}"
                                                            Style="{DynamicResource CommonButtonStyle}">
                                                        <Button.Background>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/Add.png" Stretch="Fill" />
                                                        </Button.Background>
                                                        <i:Interaction.Behaviors>
                                                            <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                                <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/Add.png" />
                                                                </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                                <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/Add.png" />
                                                                </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                                <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/Add.png" />
                                                                </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        </i:Interaction.Behaviors>

                                                    </Button>
                                                    <Button Width="15"
                                                            Height="15"
                                                            Margin="10,0,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Command="{x:Static local:ScreenPropertyPanel.CmdRemoveSenderIndex}"
                                                            Style="{DynamicResource CommonButtonStyle}">
                                                        <Button.Background>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/Remove.png" />
                                                        </Button.Background>
                                                        <i:Interaction.Behaviors>
                                                            <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                                <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/Remove.png" />
                                                                </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                                <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/Remove.png" />
                                                                </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                                <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/Remove.png" />
                                                                </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        </i:Interaction.Behaviors>

                                                    </Button>
                                                </StackPanel>
                                                <StackPanel DockPanel.Dock="Left" Orientation="Horizontal">
                                                    <Label HorizontalAlignment="Right"
                                                           VerticalAlignment="Center"
                                                           Content="{DynamicResource Lang_PortPropPanel_ModelType}"
                                                           Foreground="#FFDCDCDC" />
                                                    <local:CustomText HorizontalAlignment="Stretch"
                                                                      VerticalAlignment="Center"
                                                                      CurrentSenderConfigInfo="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                                        Path=CurrentSenderConfigInfo,
                                                                                                        Mode=TwoWay}"
                                                                      Cursor="Hand"
                                                                      ScreenRealParams="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                                 Path=ScreenRealParams,
                                                                                                 Mode=TwoWay}"
                                                                      SelectedSenderConfigInfo="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                                         Path=SelectedSenderConfigInfo,
                                                                                                         Mode=TwoWay}"
                                                                      SenderConfigCollection="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                                       Path=SenderConfigCollection,
                                                                                                       Mode=TwoWay}"
                                                                      Text="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                                     Path=CurrentSenderConfigInfo.SenderTypeName,
                                                                                     Mode=TwoWay}" />
                                                </StackPanel>
                                            </DockPanel>

                                            <Canvas Name="MySenderCanvas"
                                                    Grid.Row="1"
                                                    Width="200"
                                                    Height="160"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Stretch" />
                                        </Grid>
                                    </GroupBox>
                                    <GroupBox Grid.Row="1"
                                              Height="70"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Top"
                                              DataContext="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                    Path=ScannerCofigCollection}"
                                              Header="{DynamicResource Lang_PortPropPanel_SelectePort}">
                                        <Canvas Name="MyPortCanvas"
                                                Width="200"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Stretch" />
                                    </GroupBox>
                                    <GroupBox Grid.Row="2"
                                              Height="Auto"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Bottom"
                                              Header="{DynamicResource Lang_PortPropPanel_SelecteConnectType}">
                                        <Grid DataContext="MyTemplateSelectorDataSource">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="70" />
                                                <RowDefinition Height="70" />
                                                <RowDefinition Height="20" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="70" />
                                                <ColumnDefinition Width="70" />
                                                <ColumnDefinition Width="70" />
                                                <ColumnDefinition Width="70" />
                                            </Grid.ColumnDefinitions>
                                            <Button Grid.Row="0"
                                                    Grid.Column="0"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.LeftBottomHor}"
                                                    Content="左下水平"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_HorLeftBottom}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_B_H.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_B_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/L_B_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/L_B_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="0"
                                                    Grid.Column="1"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.RightBottomHor}"
                                                    Content="右下水平"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_HorRightBottom}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_B_H.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_B_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/R_B_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/R_B_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="0"
                                                    Grid.Column="2"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.LeftTopHor}"
                                                    Content="左上水平"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_HorLeftTop}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_T_H.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_T_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/L_T_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/L_T_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="0"
                                                    Grid.Column="3"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.RightTopHor}"
                                                    Content="右上水平"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_HorRightTop}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_T_H.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_T_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/R_T_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/R_T_H.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="1"
                                                    Grid.Column="0"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.LeftBottomVer}"
                                                    Content="左下垂直"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_VerLeftBottom}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_B_V.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_B_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/L_B_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/L_B_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="1"
                                                    Grid.Column="1"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.RightBottomVer}"
                                                    Content="右下垂直"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_VerRightBottom}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_T_V.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_B_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/R_B_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/R_B_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="1"
                                                    Grid.Column="2"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.LeftTopVer}"
                                                    Content="左上垂直"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_VerLeftTop}" />
                                                </Button.ToolTip>

                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_T_V.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/L_T_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/L_T_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/L_T_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <Button Grid.Row="1"
                                                    Grid.Column="3"
                                                    Width="60"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Command="{x:Static local:ScreenPropertyPanel.CmdArrangeScanner}"
                                                    CommandParameter="{x:Static localInterface:ArrageTypeParam.RightTopVer}"
                                                    Content="右上垂直"
                                                    Style="{DynamicResource CommonBigButtonStyle}">
                                                <Button.ToolTip>
                                                    <ToolTip Content="{DynamicResource Lang_PortPropPanel_VerRightTop}" />
                                                </Button.ToolTip>
                                                <Button.Background>
                                                    <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_T_V.png" />
                                                </Button.Background>
                                                <i:Interaction.Behaviors>
                                                    <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Disable/R_T_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.DisableImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Normal/R_T_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.NormalImage>
                                                        <Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                            <ImageBrush ImageSource="/Nova.SmartLCT.Skin;component/Image/BlueMode/Selecte/R_T_V.png" />
                                                        </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior.MouseOverImage>
                                                    </Nova_SmartLCT_Behavior:ButtonStatePictureBehavior>
                                                </i:Interaction.Behaviors>
                                            </Button>
                                            <CheckBox Grid.Row="2"
                                                      Grid.ColumnSpan="4"
                                                      Width="Auto"
                                                      Height="Auto"
                                                      HorizontalAlignment="Left"
                                                      VerticalAlignment="Center"
                                                      Command="{x:Static local:ScreenPropertyPanel.CmdManualLine}"
                                                      Content="{DynamicResource Lang_PortPropPanel_ManualConnect}"
                                                      Foreground="#FFDCDCDC"
                                                      IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                          Path=IsChecked,
                                                                          Mode=TwoWay}" />

                                        </Grid>
                                    </GroupBox>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:CustomText}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:CustomText}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <TextBlock x:Name="MyTextBlock"
                                       Width="Auto"
                                       Height="Auto"
                                       Foreground="#FFDCDCDC"
                                       Text="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                      Path=CurrentSenderConfigInfo.SenderTypeName,
                                                      Mode=TwoWay}" />
                            <Popup x:Name="Mypopup"
                                   HorizontalOffset="10"
                                   Placement="Right"
                                   PlacementTarget="{Binding ElementName=SenderTypeText}"
                                   PopupAnimation="Fade">
                                <Grid Width="120"
                                      Height="Auto"
                                      MaxHeight="300"
                                      Background="Black">
                                    <ListBox Margin="0"
                                             HorizontalAlignment="Stretch"
                                             VerticalAlignment="Stretch"
                                             Background="Black"
                                             ItemContainerStyle="{DynamicResource ListBoxItemStyle_SenderType}"
                                             ItemTemplate="{DynamicResource DataTemplate_SenderTypeList}"
                                             ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                   Path=SenderConfigCollection,
                                                                   Mode=TwoWay}"
                                             SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent},
                                                                    Path=SelectedSenderConfigInfo,
                                                                    Mode=TwoWay}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseDoubleClick">
                                                <Nova_SmartLCT_Behavior:NotifyControlEvent Command="{x:Static local:CustomText.MouseDoubleClickWithSenderType}" CommandParameters="{Binding}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </ListBox>
                                </Grid>
                            </Popup>
                        </Grid>

                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>